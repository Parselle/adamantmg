doctype html
html(lang='ru')
	head
		//- Landing-page settings
		- var $charset = 'UTF-8'

		- var $title = 'Adamant Media Group'
		- var $description = 'ADAMANT MEDIA GROUP— мы превращаем необработанный алмаз вашей бизнес-идеи в сверкающий бриллиант успешного бренда. Делаем больше, чем от нас ожидают'
		- var $url = 'http://adamantmg.ru'
		- var $keywords = 'key1, key2, key3'
		- var $tabColor = '#8e5fc2'

		- var $author = 'AdamantMG'
		- var $email = 'shtab@adamantmg.ru'

		- var $authorTwiiter = ''
		- var $authorGoogle = ''
		- var $ogImageUrl = 'http://adamantmg.ru/img/preview.jpg'

		meta(charset= $charset)
		meta(name='viewport' content='width=device-width' initial-scale=1.0)
		meta(name='theme-color' content= $tabColor)
		//- Http-equiv Tags
		meta(http-equiv='X-UA-Compatible' content='IE=edge')
		meta(http-equiv='msthemecompatible' content='no')
		meta(http-equiv='cleartype' content='on')
		meta(http-equiv='Content-Style-Type' content='text/css')
		meta(http-equiv='Content-Script-Type' content='text/javascript')
		meta(http-equiv='Cache-Control' content='no-cache')
		//- Seo
		title= $title
		meta(name='description' content= $description)
		meta(name='keywords' content= $keywords)
		meta(name='author' content= $author)
		meta(name='contact' content= $email)
		meta(http-equiv='Reply-to' content= $email)
		meta(name='copyright' content='(c)')
		//- Opengl metas
		meta(property='og:type' content='Website')
		meta(property='og:url' content= $url)
		meta(property='og:site_name' content= $title)
		meta(property='og:title' content= $title)
		meta(property='og:locale' content='ru_RU')
		meta(property='og:description' content= $description)
		meta(property='og:image' content= $ogImageUrl)
		meta(property='og:image:width' content='1200')
		meta(property='og:image:height' content='505')
		meta(name='twitter:card' content='summary')
		meta(name='twitter:description' content= $description)
		meta(name='twitter:title' content= $title)
		meta(name='twitter:site' content= $url)
		meta(name='twitter:creator' content= $authorTwiiter)
		meta(name='twitter:image' content= $ogImageUrl)
		link(href= $authorGoogle rel='publisher')
		meta(itemprop='name' content= $author)
		meta(itemprop='description' content= $description)
		meta(itemprop='image' content= $ogImageUrl)

		link(rel='icon' href='img/favicon/adamant128x128.png' sizes='128x128')
		link(rel='icon' href='img/favicon/adamant32x32.png' sizes='32x32')
		link(href='css/main.css' rel='stylesheet' )
		link(href='https://fonts.googleapis.com/css?family=Roboto:400,500,700' rel='stylesheet')
		script(async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBmbMXDyKO6qmfgN3gL4LVXAPncLcM-5jQ&amp;callback=initMap")
		script(async defer src='js/main.js')

